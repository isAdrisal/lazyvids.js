{
  "name": "lazyvids.js",
  "version": "0.0.4",
  "description": "A small utility to lazily autoplay HTML5 videos.",
  "main": "dist/lazyvids.min.js",
  "browser": "dist/lazyvids.es5.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isAdrisal/lazyvids.js.git"
  },
  "author": "isAdrisal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isAdrisal/lazyvids.js/issues"
  },
  "homepage": "https://github.com/isAdrisal/lazyvids.js#readme",
  "scripts": {
    "build": "yarn clean && mkdir -p dist && yarn build:es5 && yarn build:es6",
    "build:es5": "babel src/lazyvids.js --out-file dist/lazyvids.es5.min.js && terser dist/lazyvids.es5.min.js -o dist/lazyvids.es5.min.js",
    "build:es6": "terser src/lazyvids.js -o dist/lazyvids.min.js",
    "clean": "rimraf dist/",
    "deploy": "yarn build && yarn publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "core-js": "^3.9.1",
    "rimraf": "^3.0.2",
    "terser": "^5.6.0"
  }
}
