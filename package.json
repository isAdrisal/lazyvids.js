{
  "name": "lazyvids",
  "version": "0.2.3",
  "description": "A small utility to lazy-load autoplay HTML5 videos once they enter the viewport.",
  "main": "dist/lazyvids.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isAdrisal/lazyvids.js.git"
  },
  "author": "isAdrisal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isAdrisal/lazyvids.js/issues"
  },
  "homepage": "https://github.com/isAdrisal/lazyvids.js#readme",
  "scripts": {
    "build": "yarn clean && esbuild src/lazyvids.js --outdir=dist --minify --target=safari12",
    "deploy": "yarn build && yarn publish",
    "clean": " rm -rf dist/"
  },
  "devDependencies": {
    "esbuild": "^0.12.25"
  }
}
